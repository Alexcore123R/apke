<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="16.0dip" />
            <solid android:color="@color/shopping_cart_pressed_yellow_color" />
            <stroke android:width="2.0dip" android:color="@color/shopping_cart_pressed_yellow_color" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="16.0dip" />
            <solid android:color="@color/shopping_cart_yellow_color" />
            <stroke android:width="2.0dip" android:color="@color/shopping_cart_yellow_color" />
        </shape>
    </item>
</selector>