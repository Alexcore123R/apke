<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <solid android:color="@color/order_confirm_goods_number_pressed_bg" />
            <stroke android:width="0.5dip" android:color="@color/order_confirm_num_selector_new_stroke_color" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <solid android:color="@color/order_confirm_light_text_color" />
            <stroke android:width="0.5dip" android:color="@color/order_confirm_num_selector_new_stroke_color" />
        </shape>
    </item>
</selector>